package com.test;

public class StringSizeWithoutInbuiltFunction {

	public static void main(String[] args) {
		String sr = "Swar";
		
		approach1(sr);
		
		approach2(sr);
				
	}

	private static void approach2(String sr) {
		try 
		{
			int i = 0;
			
		} catch (Exception e) 
		{
			
		}
	}

	private static void approach1(String sr) 
	{
		int count = 0;
		char arr[] = sr.toCharArray();
		for (char c : arr) 
		{
			System.out.print(c);
			count++;
		}
			System.out.println(" " +count);
	} 		
}
