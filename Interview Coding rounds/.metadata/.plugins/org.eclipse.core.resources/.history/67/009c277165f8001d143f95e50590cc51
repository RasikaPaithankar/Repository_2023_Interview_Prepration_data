package com.test;

import java.util.HashSet;

public class StringAnagram {

	public static void main(String[] args) {
		String sr = "Angle";
		String sr2 = "Angel";
		
		HashSet<Character> ht = new HashSet<Character>();
		char ch;
		for(int i = 0; i <= sr.length()-1; i++) {
			ch = sr.charAt(i);
			ht.add(ch);
		}
		
		System.out.println(ht);
	}
}
