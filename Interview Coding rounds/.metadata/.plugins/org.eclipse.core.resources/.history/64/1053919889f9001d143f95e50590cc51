package com.test;

public class StringSizeWithoutInbuiltFunction {

	public static void main(String[] args) {
		try 
		{
			String sr = "Swar";
			int count = 0;
			char arr[] = sr.toCharArray();
			for (char c : arr) {
				System.out.println(c);
				count++;
			}
			System.out.println(count);
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		
		
	}

}
