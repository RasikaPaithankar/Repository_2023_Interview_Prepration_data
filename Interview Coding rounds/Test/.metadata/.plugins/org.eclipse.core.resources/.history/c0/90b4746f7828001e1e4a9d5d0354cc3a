package com.test;

public class A {
	/*
	 * private static int t1 = 1; private static int t2 = 2; private int t4 = 4;
	 * private int t5 = 5;
	 * 
	 * public class inner { private static int t3 = 3;
	 * 
	 * private static int sum() { return (t1+t2); } }
	 */
	
	
	public static void main(String[] args) {
		/*
		 * A.inner a = new A.inner();
		 */ 

		String s1 = "Java";
		String s2 = s1.intern();
		String s3 = "Java";
		String s4 = new String("Java");
		StringBuffer sb = new StringBuffer("Java");
		
		System.out.println((s1==s2) + " " + (s2==s3)+ " " + (s3==s4) + " "+ sb.equals(1)+ " " + s1.equals(sb));
	
		
		byte z = 4;
		System.out.println(z<<4);
		System.out.println(z * (2^4));
	}
}
