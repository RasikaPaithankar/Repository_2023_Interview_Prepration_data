package com.test;

public class Test {

	public static void main(String[] args) {
		int x = -4;
		System.out.print(x>>1);
		System.out.print(" And ");
		int y = 4;
		System.out.print(y>>2);
		
		int[] a = new int[10];
		byte[] b = new byte[10];
		char[] c = new char[10];
		double[] d = new double[10];
		
		System.out.println(a[0] + " " + b[0]+ " "+ c[0] + " "+d[0]);
	}

}
