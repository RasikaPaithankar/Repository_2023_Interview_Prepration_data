package com.data.demo;

public class Test2 {

	public static void main(String[] args) {
		
		for(int j = 0; j <= 5; j++) 
		{
			for(int i = 5; i>=1; i--) 
			{
				
				System.out.println(" ");
			}
			for(int k = 1; k <=5; k++) {
				System.out.println("*");
			}
			
		}

}

	
