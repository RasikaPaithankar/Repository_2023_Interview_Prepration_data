package com.data.demo;

public class Test2 {

	public static void main(String[] args) {
		
		for(int j = 1; j <= 5; j++) 
		{
			for(int k= 1; k<=5; k++) 
			{
				while(k <= 5-1) 
				{
					System.out.println(" ");
				}
				
			}
			
		}

}

	
